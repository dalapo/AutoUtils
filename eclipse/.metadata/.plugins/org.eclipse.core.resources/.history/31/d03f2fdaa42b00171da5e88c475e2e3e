package dalapo.autoutils;

import net.minecraftforge.Mod;

@Mod(modid="AutoUtils", name="Automation Utilities", version="0.0.1")
public class AutoUtils
{
	public static final Block[] blocks = new Block[BlockRegistry.getSize()];
	public static final Item[] items = new Item[ItemRegistry.getSize()];
	
	@SidedProxy(clientSide="dalapo.autoutils.ClientProxy", serverSide="dalapo.autoutils.CommonProxy");
	public static CommonProxy proxy;
}